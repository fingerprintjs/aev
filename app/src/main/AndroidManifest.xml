<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fingerprintjs.android.aev.demo">

    <uses-sdk tools:overrideLibrary="androidx.security.ktx" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlaygroundPRO"
        tools:ignore="LockedOrientationActivity,UnusedAttribute">
        <activity
            android:name="com.fingerprintjs.android.aev.demo.WelcomeActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fingerprintjs.android.aev.demo.TermsActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.fingerprintjs.android.aev.demo.demo_screen.DemoActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>
</manifest>